{
  "extends": ["airbnb-typescript-prettier", "plugin:jest/recommended"],
  "env": {
    "jest/globals": true
  },
  "plugins": ["jest"],
  "rules": {
    "import/prefer-default-export": "off",
    "@typescript-eslint/ban-ts-ignore": "off",
    "react/jsx-props-no-spreading": "warn",
    "no-return-assign": "warn"
  }
}
